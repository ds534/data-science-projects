#1
F=input("F değerini giriniz:",)
F=int(F)
C = (5/9) * (F - 32)
print(C,"C") ### rakamlar C arasındaki boşluğu nasıl gideririm?


C=input("C değerini giriniz:",)
C=int(C)
F=C/(5/9)+32
print(F,"F")

#2
year=input("year of service",)
year=int(year)
print(year)
salary=input("your salary",)

if year>5:
  print("your bonus",salary*1.05)
else:
  print("you cant take bonus")

