#3
yas1=input("yaşınızı giriniz",)
yas1=int(yas1)

yas2=input("yaşınızı giriniz",)
yas2=int(yas2)

yas3=input("yaşınızı giriniz",)
yas3=int(yas3)

yas=[yas1,yas2,yas3]
yas.sort()
print(yas)

youngest=yas[0]
oldest=yas[-1]

print(youngest)
print(oldest)


#4
held=input("Number of classes held",)
held=int(held)
attended=input("Number of classes attended",)
attended=int(attended)

if (attended/held)>0.75:
  print("allowed")
else:
  print("bye")

